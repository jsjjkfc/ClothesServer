	<script type="text/javascript">
		var proid = '${proid}';
		var isLogin = 0;
		var cartSource = '';
		var isSoldout = parseInt('0'), isStockUpdate = true;
		function saveProValue() {
			return {
				product_id : proid,
				brand_id : '327514',
				filterSize : '0',
				is_old : '0'
			}
		};
		$
				.ajax({
					type : "get",
					url : "/ClothesServer/product/productinfo?id=" + proid,
					success : function(msg) {
						$('.u-hd-tit>span').text(msg.name);
						$('h1').text(msg.name);
						$('.g_d_price').text('¥' + msg.salePrice);
						$('.fontstyle.prf05').text('¥' + msg.price);
						$('.dc_table.fst').find('tr:first').find('td:eq(1)')
								.text(msg.supplier.desc);
						$('.dc_table.fst').find('tr:eq(1)').find('td:eq(1)')
								.text(msg.name);
						var pics = msg.picPathes;
						$('[data-cartimg="img"]').text('');
						$('.dc_img_detail>p').text('');
						$
								.each(
										pics,
										function(i, pic) {
											var html = '<li style="width: 238px; display: table-cell; vertical-align: top;"><span><img src="'+pic+'" alt=""></span></li>';
											$('[data-cartimg="img"]').append(
													html);
											var show = '<img src="'+pic+'" width="320">';
											$('.dc_img_detail>p').append(show);
										});

					}
				});
		$("a[data-action=addNum]").click(function() {
			$("a[data-action=minNum]").removeClass("disabled");
			var num = $(".amount-text").text();
			$(".amount-text").text(parseInt(num) + 1);
			$(".amount-num").attr("value", parseInt(num) + 1);
		});
		$("a[data-action=minNum]").click(function() {
			var num = $(".amount-text").text();
			if (num == 1) {
				$(".amount-text").text(1);
				$(".amount-num").attr("value", 1);
				$("a[data-action=minNum]").addClass("disabled");
			} else {
				$(".amount-text").text(parseInt(num) - 1);
				$(".amount-num").attr("value", parseInt(num) - 1);
			}
			setOrderInfo();
		});
		function setOrderInfo() {
			var productCount = $(".amount-text").text();
			var href = "/ClothesServer/product/add?proid=${proid}&count="
					+ productCount;
			$("#addToChart").attr("href", href);
		}
	</script>
