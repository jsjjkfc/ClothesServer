<script type="text/javascript">
	$(".cart.cell").click(function() {
		$("#s-decision-wrapper").find("h1").text("添加购物车");
		$("#s-decision-wrapper").css("display", "block").addClass("show");
		$("body").css("-webkit-tap-highlight-color", "transparent");
		$(".ok").css("display", "");
		$(".option").find(".buy").css("display", "none");
		$(".option").find(".cart").css("display", "none");
	});
	$(".buy.cell").click(function() {
		$("#s-decision-wrapper").find("h1").text("立即购买");
		$("#s-decision-wrapper").css("display", "block").addClass("show");
		$("body").css("-webkit-tap-highlight-color", "transparent");
		$(".ok").css("display", "");
		$(".option").find(".buy").css("display", "none");
		$(".option").find(".cart").css("display", "none");
	});
	$("#s-action-container").click(function() {
		$("#s-decision-wrapper").find("h1").text("选择商品信息");
		$("#s-decision-wrapper").css("display", "block").addClass("show");
		$("body").css("-webkit-tap-highlight-color", "transparent");
		$(".ok").css("display", "none");
		$(".option").find(".buy").css("display", "");
		$(".option").find(".cart").css("display", "");
	});
	$(".back").click(function() {
		$("#s-decision-wrapper").css("display", "none").removeClass("show");
		$("body").css("-webkit-tap-highlight-color", "");
	});
	var num = $(".mui-number").find(".num");
	$(".mui-number").find(".increase").click(function() {
		var oldnum = num.val();
		num.val(parseInt(oldnum) + 1);
		num.attr("value", parseInt(oldnum) + 1);
	});
	$(".mui-number").find(".decrease").click(function() {
		var oldnum = num.val();
		if (oldnum == 1) {
			return;
		} else {
			num.val(parseInt(oldnum) - 1);
			num.attr("value", parseInt(oldnum) - 1);
		}
	});
</script>
