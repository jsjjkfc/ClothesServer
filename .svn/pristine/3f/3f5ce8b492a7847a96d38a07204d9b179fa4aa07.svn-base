	font-size: 24px;
	line-height: 44px
}

.mui-page-in-app .mui-header-search {
	background-color: #000;
	height: 30px;
	position: relative;
	top: 7px;
	float: none;
	width: auto;
	border-radius: 2px;
	padding-left: 9px;
	line-height: 30px;
	text-align: left;
	font-size: 14px;
