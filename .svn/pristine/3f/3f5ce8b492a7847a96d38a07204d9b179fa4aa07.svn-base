	line-height: 44px;
	background: #fff;
	color: #5F646E;
	text-align: center;
	font-size: 24px
}

.mui-header-global-nav, .mui-header-city-selector {
	float: right;
	height: 44px
}

.mui-header-category, .mui-header-search {
	float: left;
	height: 44px
}

.mui-header-global-nav {
	padding-right: 15px
}

.mui-header-global-nav::before {
	content: '\e605';
