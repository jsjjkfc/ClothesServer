package com.twocore.bean.resp;

import java.util.ArrayList;
import java.util.List;

import com.twocore.model.SupplierModel;
import com.twocore.model.TypeModel;

public class RespProductData {

	private int productId;
	private SupplierModel supplier;
	private String name;
	private String desc;
	private float price;
	private float salePrice;
	private TypeModel type;
	private List<String> picPathes = new ArrayList<String>();

	public int getProductId() {
		return productId;
	}

	public void setProductId(int productId) {
		this.productId = productId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public float getPrice() {
		return price;
	}

	public void setPrice(float price) {
		this.price = price;
	}

	public float getSalePrice() {
		return salePrice;
	}

	public void setSalePrice(float salePrice) {
		this.salePrice = salePrice;
	}

	public SupplierModel getSupplier() {
		return supplier;
	}

	public void setSupplier(SupplierModel supplier) {
		this.supplier = supplier;
	}

	public List<String> getPicPathes() {
		return picPathes;
	}

	public void setPicPathes(List<String> picPathes) {
		this.picPathes = picPathes;
	}

	public TypeModel getType() {
		return type;
	}

	public void setType(TypeModel type) {
		this.type = type;
	}
}
