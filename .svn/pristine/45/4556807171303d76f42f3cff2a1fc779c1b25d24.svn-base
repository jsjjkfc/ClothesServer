	<script type="text/javascript">
		$(".my-info.cell.fixed").click(function() {
			$("#J_MyInfoDialog").css("display", "block");
			$("#J_MyInfoMask").css("display", "block");
		});
		$("#J_MyInfoClose").click(function() {
			$("#J_MyInfoDialog").css("display", "none");
			$("#J_MyInfoMask").css("display", "none");
		});
		$(document)
				.ready(
						function($) {
							$(document).scroll(
									function() {
										var top = $(document).scrollTop();
										if (top < 100)
											$("#J_HeaderContent").removeClass(
													"tmall-bg-red");
										else {
											$("#J_HeaderContent").addClass(
													"tmall-bg-red");
										}
									});
							$
									.getJSON(
											"/ClothesServer/home/data",
											function(data) {
												$
														.each(
																data,
																function(i,
																		item) {
																	var html = '<div class="item">'
																			+ '				<div class="caption">'
																			+ '					<h3 class="caption_life">'
																			+ '						<a href="">'
																			+ item.title
																			+ '</a>'
																			+ '					</h3>'
																			+ '				</div>'
																			+ '				<div class="item_brand">'
																			+ '					<a href="javascript:;" '+
													'						> <img class="fadeInImg"'+
													'						src="'+item.picTop+'">'
																			+ '					</a>'
																			+ '				</div>'
																			+ '				<div class="item_con clearfix">'
																			+ '					<dl><dt>'
																			+ '							<a href="/ClothesServer/product/productdetail?id='
																			+ item.goodOneId
																			+ '"><img'+
													'								 class="fadeInImg"'+
													'								src="'+item.goodOnePic+'"></a>'
																			+ '						</dt>'
																			+ '						<dd>'
																			+ '							<p class="txt">'
																			+ item.goodOneTitle
																			+ '</p>'
																			+ '							<div class="price clearfix">'
																			+ '								<span class="now_price"><i>￥</i><em>'
																			+ item.goodOneSalePrice
																			+ '</em></span><span'+
													'									class="old_price"><i>￥</i><em>'
																			+ item.goodOnePrice
																			+ '</em></span>'
																			+ '							</div>'
																			+ '						</dd>'
																			+ '					</dl>'
																			+ '					<dl>'
																			+ '						<dt>'
																			+ '							<a href="/ClothesServer/product/productdetail?id='
																			+ item.goodOneId
																			+ '"'
																			+ '								><img'+
													'								class="fadeInImg"'+
													'								src="'+item.goodTwoPic+'"></a>'
																			+ '						</dt>'
																			+ '						<dd>'
																			+ '							<p class="txt">'
																			+ item.goodTwoTitle
																			+ '</p>'
																			+ '							<div class="price clearfix">'
																			+ '								<span class="now_price"><i>￥</i><em>'
																			+ item.goodTwoSalePrice
																			+ '</em></span><span'+
													'									class="old_price"><i>￥</i><em>'
																			+ item.goodTwoPrice
																			+ '</em></span>'
																			+ '							</div>'
																			+ '						</dd>'
																			+ '					</dl>'
																			+ '				</div>'
																			+ '			</div>';
																	$(html)
																			.appendTo(
																					'.wrap');
																});
												var items = $(".item");
												$(".item")
														.each(
																function(i) {
																	var item = items
																			.eq(i);
																	item
																			.find(
																					"a:first")
																			.text(
																					data[i].title);
																	item
																			.find(
																					".now_price")
																			.eq(
																					0)
																			.text(
																					"")
																			.append(
																					"<i>￥</i><em>"
																							+ data[i].goodOneSalePrice
																							+ "</em>");
																	item
																			.find(
																					".old_price")
																			.eq(
																					0)
																			.text(
																					"")
																			.append(
																					"<i>￥</i><em>"
																							+ data[i].goodOnePrice
																							+ "</em>");
																	item
																			.find(
																					".now_price")
																			.eq(
																					1)
																			.text(
																					"")
																			.append(
																					"<i>￥</i><em>"
																							+ data[i].goodTwoSalePrice
																							+ "</em>");
																	item
																			.find(
																					".old_price")
																			.eq(
																					1)
																			.text(
																					"")
																			.append(
																					"<i>￥</i><em>"
																							+ data[i].goodTwoPrice
																							+ "</em>");
																	item
																			.find(
																					".txt:first")
																			.text(
																					data[i].goodOneTitle);
																	item
																			.find(
																					".txt")
																			.eq(
																					1)
																			.text(
																					data[i].goodTwoTitle);
																	item
