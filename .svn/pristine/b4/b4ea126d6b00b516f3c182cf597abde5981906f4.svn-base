package com.twocore.bean.resp;

/**
 * 首页商品展示列表单个数据
 * 
 * @author lupeng
 *
 */

public class GoodsListModel {

	// 板块标题
	private String title;
	// 公司的图片路径
	private String picTop;
	// 左下商品名称
	private String goodOneTitle;
	// 左下商品原价
	private float goodOnePrice;
	// 图片
	private String goodOnePic;
	private String goodTwoPic;
	// 左下商品促销价
	private float goodOneSalePrice;
	// 右下商品名称
	private String goodTwoTitle;
	// 右下商品原价
	private float goodTwoPrice;
	// 右下商品促销价
	private float goodTwoSalePrice;
	// 商品一的id
	private String goodOneId;
	// 商品二的id
	private String goodTwoId;

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getPicTop() {
		return picTop;
	}

	public void setPicTop(String picTop) {
		this.picTop = picTop;
	}

	public String getGoodOneTitle() {
		return goodOneTitle;
	}

	public void setGoodOneTitle(String goodOneTitle) {
		this.goodOneTitle = goodOneTitle;
	}

	public float getGoodOnePrice() {
		return goodOnePrice;
	}

	public void setGoodOnePrice(float goodOnePrice) {
		this.goodOnePrice = goodOnePrice;
	}

	public float getGoodOneSalePrice() {
		return goodOneSalePrice;
	}

	public void setGoodOneSalePrice(float goodOneSalePrice) {
		this.goodOneSalePrice = goodOneSalePrice;
	}

	public String getGoodTwoTitle() {
		return goodTwoTitle;
	}

	public void setGoodTwoTitle(String goodTwoTitle) {
		this.goodTwoTitle = goodTwoTitle;
	}

	public float getGoodTwoPrice() {
		return goodTwoPrice;
	}

	public void setGoodTwoPrice(float goodTwoPrice) {
		this.goodTwoPrice = goodTwoPrice;
	}

	public float getGoodTwoSalePrice() {
		return goodTwoSalePrice;
	}

	public void setGoodTwoSalePrice(float goodTwoSalePrice) {
		this.goodTwoSalePrice = goodTwoSalePrice;
	}

	public String getGoodOnePic() {
		return goodOnePic;
	}

	public void setGoodOnePic(String goodOnePic) {
		this.goodOnePic = goodOnePic;
	}

	public String getGoodTwoPic() {
		return goodTwoPic;
	}

	public void setGoodTwoPic(String goodTwoPic) {
		this.goodTwoPic = goodTwoPic;
	}

	public String getGoodOneId() {
		return goodOneId;
	}

	public void setGoodOneId(String goodOneId) {
		this.goodOneId = goodOneId;
	}

	public String getGoodTwoId() {
		return goodTwoId;
	}

	public void setGoodTwoId(String goodTwoId) {
		this.goodTwoId = goodTwoId;
	}
}
